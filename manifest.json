{
  "name": "Cryptr",
  "short_name": "Cryptr",
  "version": "1.2",
  "description": "Keep your data safe with easy encryption.",
  "manifest_version": 2,
  "permissions": [ "storage", "contextMenus", "tabs" ],
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "cryptojs/rollups/aes.js",
      "cryptojs/rollups/rabbit.js",
      "cryptojs/rollups/rc4.js",
      "cryptojs/rollups/tripledes.js",
      "cryptojs/lib-typedarrays.js",
      "script.js"
    ]
  },
  "browser_action": { "default_icon": "img/icon19.png" },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "icons": { "16": "img/icon16.png", "48": "img/icon48.png", "128": "img/icon128.png" },
  "homepage_url": "http://www.cryptr.ca/"
}
